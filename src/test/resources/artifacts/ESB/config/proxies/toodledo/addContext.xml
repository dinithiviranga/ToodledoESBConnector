<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="addContext"
       transports="https http"
       startOnLoad="true"
       trace="disable">
   <description/>
   <target>
      <inSequence>
         <property name="name" expression="json-eval($.name)"/>
         <property name="private" expression="json-eval($.private)"/>
         <property name="access_token" expression="json-eval($.access_token)"/>
         <ToodleDo.addingContext>
            <name>{$ctx:name}</name>
            <private>{$ctx:private}</private>
             <ToodleDo.getAccessToken>
                 <access_token>{$ctx:access_token}</access_token>
             </ToodleDo.getAccessToken>
         </ToodleDo.addingContext>
         <respond/>
      </inSequence>
      <outSequence>
         <log/>
         <send/>
      </outSequence>
   </target>
</proxy>
